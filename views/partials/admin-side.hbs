<!-- Layout wrapper -->
<div>

  <!-- Menu -->

  <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme sidebar-main-div">
    <div class="app-brand demo">
      <a href="/admin" class="app-brand-link">
        <span class="app-brand-logo demo">
          <img class="logo-icon" src="/images/icon/logo.png" alt="Logo">
        </span>
        <span class="app-brand-text demo menu-text fw-bold ms-2">Bristles</span>
      </a>

      <a href="javascript:void(0);" class="layout-menu-toggle menu-link text-large ms-auto d-block d-xl-none">
        <i class="bx bx-chevron-left bx-sm align-middle"></i>
      </a>
    </div>

    <div class="menu-inner-shadow"></div>

    <ul class="menu-inner py-1">
      <!-- Dashboard -->
      <li class="menu-item " id="dashboard">
        <a href="/admin" class="menu-link">
          <i class="menu-icon tf-icons bx bxs-pie-chart-alt-2"></i>
          <div data-i18n="Analytics">Dashboard</div>
        </a>
      </li>
      <li class="menu-item " id="carousel">
        <a href="/admin/carousel" class="menu-link">
          <i class="menu-icon tf-icons bx bxs-carousel"></i>
          <div data-i18n="Analytics">Carousel</div>
        </a>
      </li>

      <!-- Layouts -->
      <li class="menu-item " id="products">
        <a href="javascript:void(0);" class="menu-link menu-toggle">
          <i class="menu-icon tf-icons bx bxs-image"></i>
          <div data-i18n="Layouts">Products</div>
        </a>

        <ul class="menu-sub">
          {{#each CAT}}
          <li class="menu-item" id="pro{{this.title}}">
            <a href="/admin/products/{{this.title}}" class="menu-link">
              <div data-i18n="Without menu">{{this.title}}</div>
            </a>
          </li>
          {{/each}}


        </ul>
      </li>


      <li class="menu-item" id="pendingPro">
        <a href="javascript:void(0);" class="menu-link menu-toggle">
          <i class="menu-icon tf-icons bx bxs-hourglass-top"></i>
          <div data-i18n="Account Settings">Pending Products</div>
        </a>
        <ul class="menu-sub">
          {{#each CAT}}
          <li class="menu-item" id="pnt{{this.title}}">
            <a href="/admin/pending-products/{{this.title}}" class="menu-link">
              <div data-i18n="Blank">{{this.title}}</div>
            </a>
          </li>
          {{/each}}
        </ul>
      </li>

      <li class="menu-item" id="category">
        <a href="/admin/category" class="menu-link">
          <i class="menu-icon tf-icons bx bxs-category-alt"></i> 
          <div data-i18n="Basic">Category</div>
        </a>
      </li>
      <li class="menu-item" id="coupon">
        <a href="/admin/coupon" class="menu-link">
          <i class="menu-icon tf-icons bx bxs-coupon"></i>
          <div data-i18n="Basic">Coupon</div>
        </a>
      </li>

      <li class="menu-item" id="artist">
        <a href="javascript:void(0);" class="menu-link menu-toggle">
          <i class="menu-icon tf-icons bx bxs-brush"></i>
          <div data-i18n="Account Settings">Artists</div>
        </a>
        <ul class="menu-sub">

          <li class="menu-item" id="artistOk">
            <a href="/admin/artist/all-artist" class="menu-link">
              <div data-i18n="Blank">All Artist</div>
            </a>
          </li>
          <li class="menu-item" id="artistNot">
            <a href="/admin/artist/new-account" class="menu-link">
              <div data-i18n="Blank">New Accounts</div>
            </a>
          </li>

        </ul>
      </li>
      <li class="menu-item" id="user">
        <a href="/admin/user-list" class="menu-link">
          <i class="menu-icon tf-icons bx bxs-user-circle"></i>
          <div data-i18n="Basic">Users</div>
        </a>
      </li>
      <li class="menu-item" id="order">
        <a href="/admin/order-list" class="menu-link">
          <i class="menu-icon tf-icons bx bxs-package"></i>
          <div data-i18n="Basic">Orders</div>
        </a>
      </li>

    </ul>
  </aside>
  <!-- / Menu -->


</div>



<!-- / Layout wrapper -->